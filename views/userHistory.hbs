<div class="row">
    <div class="col-md-12">
        <h2>{{session.user.userName}} ({{session.user.email}}) History</h2>

        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Login Date/Time</th>
                        <th>Client Information</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    {{#each session.user.loginHistory}}
                    <tr>
                        <td>{{dateTime}}</td>
                        <td>{{userAgent}}</td>
                    </tr>

                    {{else}}
                    <tr>
                        <td>No Data Available</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>